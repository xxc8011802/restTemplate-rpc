<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


    <bean id="user" class="com.example.client.entity.User"></bean>
    <bean id="userService" class="com.example.client.service.UserServiceImpl"></bean>
    <bean id="serviceProxy" class="com.example.client.rpcproxy.ServiceProxy"></bean>
    <bean id="httpClientService" class="com.example.client.http.HttpClientService"></bean>
</beans>